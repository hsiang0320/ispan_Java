package hw2;

import java.util.Scanner;

public class Factor {

	public static void main(String[] args) {
		int num;
		Scanner scanner = new Scanner(System.in);
		System.out.println("請輸入一個整數：");
		num = scanner.nextInt();
		for(int i=1;i<=num/2;i++) {
			if(num % i==0) {
				System.out.print(i);
			}
		}
	}

}
