package hw1;

public class Salary {

	public static void main(String[] args) {
		int workingHours = 90;
		int salary=0;
		int hourlyWage =150;
		if(workingHours>60) {
			salary=hourlyWage*60;
			if(workingHours<=80) {
				salary=(int) (salary+(hourlyWage-60)*workingHours*1.25);
			}if(workingHours>80) {
				
			}
		
		}else {
			salary=hourlyWage*60;
		}
		

	}

}
