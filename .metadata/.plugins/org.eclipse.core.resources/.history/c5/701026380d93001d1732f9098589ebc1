package hw4;

public class Verifier {
	static void f1() {
		for (int i = 0; i < 4; i++) {
			System.out.print((int) (Math.random() * 10));
		}
		System.out.println();
	}

	static void f2() {
		int[] check = new int[4];
		int n=0;
		do {
			check[n]=(int) (Math.random() * 10);
			for(int i =0;i<n;i++) {
				if(check[i]!=check[n]) {
					n++;
				}
			}
			
		} while (n<3);

	}

	public static void main(String[] args) {
		f1();// 第一題
		f2();
	}

}
