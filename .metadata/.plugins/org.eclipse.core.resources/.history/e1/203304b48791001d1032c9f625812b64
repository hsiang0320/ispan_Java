package hw2;

public class Stars {
	public static void main(String[] args) {
		for (int i = 1; i < 6; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		System.out.println();
		System.out.println();
		for (int i = 5; i >= 1; i--) {
			for (int j = 1; j <= i; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		System.out.println();
		System.out.println();
		for (int i = 1; i <= 5; i++) {
			for (int j = 4; j <= (5-i); j--) {
				System.out.println();
				System.out.print(" ");
			}
			System.out.print("*");
		}
	}
}
