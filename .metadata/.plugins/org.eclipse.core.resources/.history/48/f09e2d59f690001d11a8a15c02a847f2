package hw1;

public class Equation {

	public static void main(String[] args) {
		int a = 5;
		int b = 3;
		int c = 9;
		if (Math.pow(b,2)-4*a*c>0) {
			System.out.println("有兩個不相等實根");
		}else if (Math.pow(b,2)-4*a*c==0) {
			System.out.println("有兩個相等實根");
		}else if (Math.pow(b,2)-4*a*c==0) {
			System.out.println("沒有實根");
		}
	}

}
