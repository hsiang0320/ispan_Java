package hw3;

public class Sales {

	public static void main(String[] args) {
		String [] employee = {"Jean","Tom","Tina"};
		int[][] productSales = { { 33, 32, 56, 45, 33 },
								{ 77, 33, 68, 45, 23 },
								{ 43, 55, 43, 67, 65 } };
		int [] productPrice= {12,16,10,14,15};
		int e_sales=0;
		for(int i=0;i<productSales.length;i++) {
			e_sales=0;
			for(int j =0;j<productSales[i].length;j++) {
				e_sales=e_sales+productPrice[j]*productSales[i][j];
			}
			System.out.println(employee[i]+"的銷售總額為:"+e_sales);
			}

	}

}
