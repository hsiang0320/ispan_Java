package hw2;

import java.nio.channels.InterruptedByTimeoutException;
import java.util.Scanner;

public class PerfectNumber {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 100; i++) {
			sum = 0;
			for (int j = 1; j <= i; j++) {
				if (i % j == 0) {
					sum += j;
					System.out.print(i + ",");
				}
			}
		}
	}

}
